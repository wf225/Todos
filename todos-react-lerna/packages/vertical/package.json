{
  "name": "todos-vertical",
  "version": "0.1.1",
  "description": "Todos-vertical",
  "main": "index.js",
  "author": "Bill Wu",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "^5.0.4",
    "react-slot-fill": "1.0.0-alpha.10",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.6",
    "redux-thunk": "^2.2.0",
    "todos-platform": "^0.1.1"
  },
  "devDependencies": {},
  "config": {
    "s3BucketName": "dev-wubil-todos",
    "region": "us-east-1",
    "cloudFormationStackName": "wubil-todos-test-dev",
    "functionName": "YOUR_SERVERLESS_EXPRESS_LAMBDA_FUNCTION_NAME",
    "accountId": "YOUR_ACCOUNT_ID"
  },  
  "scripts": {
    "prebuild": "webpack --config webpack.dll.config.js",
    "build": "npm run webpack-prod",
    "build-dev": "npm run webpack-dev",
    "create-bucket": "node config/s3_createbucket.js %npm_package_config_s3BucketName% && node config/s3_setbucketwebsite.js %npm_package_config_s3BucketName% index.html error.html",      
    "deploy": "aws s3 sync ./dist s3://%npm_package_config_s3BucketName% --delete --acl public-read",
    "test": "jest -u --coverage",
    "webpack-dev": "webpack --config webpack.dev.js",
    "webpack-prod": "webpack --config webpack.prod.js"
  },
  "jest": {
    "setupFiles": [
      "./browserMocks.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}